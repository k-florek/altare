{
  "name": "altare",
  "version":"1.0.0",
  "auspice-version": "2.15.0",
  "description": "Desktop app for visualizing pathogen evolution",
  "author": "Kelsey Florek",
  "license": "AGPL-3.0-only",
  "repository": "github.com/k-florek/altare",
  "homepage": "https://github.com/k-florek/altare",
  "engines": {
    "node": "10.8.x",
    "npm": "6.2.x"
  },
  "bin": {
    "auspice": "./auspice.js"
  },
  "main": "./electronDesktop/electronApp.js",
  "scripts": {
    "create-data-dir": "env bash ./scripts/create-data-dir.sh",
    "postinstall": "npm run create-data-dir",
    "view": "node auspice.js view --verbose",
    "dev": "node auspice.js develop --verbose",
    "start": "npm run view",
    "start-electron": "electron ./electronDesktop/electronApp.js .",
    "build-electron": "node auspice.js build --verbose --extend ./electronDesktop/client_customisation/config.json",
    "server": "npm run view",
    "build": "node auspice.js build --verbose",
    "prepare": "npm run build",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "get-data": "env bash ./scripts/get-data.sh",
    "get-narratives": "env bash ./scripts/get-narratives.sh",
    "heroku-postbuild": "npm run build && npm run get-data && npm run get-narratives",
    "rebuild-docker-image": "env bash ./scripts/rebuild-docker-image.sh",
    "gzip-and-upload": "env bash ./scripts/gzip-and-upload.sh",
    "build-docs": "echo 'see ./docs-src/README.md'",
    "test": "jest test/*.js",
    "integration-test": "NODE_ENV=test ENV=dev jest ./test/integration/*.js --config puppeteer.config.js",
    "integration-test:ci": "start-server-and-test server http://localhost:4000 integration-test",
    "smoke-test": "NODE_ENV=test ENV=dev jest ./test/smoke-test/urls.test.js --config puppeteer.config.js",
    "smoke-test:ci": "start-server-and-test server http://localhost:4000 smoke-test",
    "diff-lang": "./scripts/diff-lang.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --asar --arch=x64 --icon=electronDesktop/icons/favicon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . altare --overwrite --asar --platform=win32 --arch=ia32 --icon=electronDesktop/icons/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"altare\"",
    "package-linux": "electron-packager . altare --overwrite --asar --platform=linux --arch=x64 --icon=electronDesktop/icons/favicon.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.10.1",
    "argparse": "^1.0.10",
    "awesomplete": "^1.1.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-strip-function-call": "^1.0.2",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "binomial": "^0.2.0",
    "bundlesize": "^0.18.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "compression-webpack-plugin": "^3.1.0",
    "create-react-class": "^15.6.0",
    "css-loader": "^2.1.1",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.6",
    "d3-brush": "^1.1.5",
    "d3-collection": "^1.0.4",
    "d3-color": "^1.4.1",
    "d3-ease": "^1.0.6",
    "d3-format": "^1.4.4",
    "d3-interpolate": "^1.4.0",
    "d3-scale": "^1.0.5",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-timer": "^1.0.10",
    "d3-transition": "^1.3.2",
    "d3-zoom": "^1.8.3",
    "dom-to-image": "^2.6.0",
    "dompurify": "^2.0.11",
    "electron-default-menu": "^1.0.1",
    "electron-packager": "^14.2.1",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "~1.0.0",
    "es6-promise-polyfill": "^1.2.0",
    "express": "^4.16.3",
    "express-naked-redirect": "^0.1.7",
    "express-static-gzip": "^0.2.2",
    "file-loader": "^1.1.11",
    "i18next": "^19.4.5",
    "i18next-resource-store-loader": "^0.1.2",
    "json-loader": "^0.5.1",
    "leaflet": "^1.6.0",
    "leaflet-gesture-handling": "^1.1.8",
    "linspace": "^1.0.0",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "mousetrap": "^1.6.5",
    "node-fetch": "^2.1.2",
    "outer-product": "0.0.4",
    "papaparse": "^4.3.5",
    "prettyjson": "^1.2.1",
    "prop-types": "^15.6.0",
    "query-string": "^4.2.3",
    "react": "^16.13.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.21",
    "react-i18next": "^11.5.0",
    "react-icons": "^3.10.0",
    "react-redux": "^5.1.2",
    "react-select": "^1.0.0-rc.5",
    "react-tweet-embed": "^1.1.0",
    "redux": "^4.0.5",
    "redux-devtools": "^3.5.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "run-sequence": "~0.3.6",
    "style-loader": "^0.13.2",
    "styled-components": "^4.4.1",
    "typeface-lato": "^0.0.75",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.24.3",
    "whatwg-fetch": "^0.10.1",
    "yaml-front-matter": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "electron": "^9.0.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-defaults": "^7.0.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.5.4",
    "jest-circus": "^25.5.4",
    "jest-image-snapshot": "^3.1.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.1.1",
    "quoted-printable": "^1.0.1",
    "start-server-and-test": "^1.11.0",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
